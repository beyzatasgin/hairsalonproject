    <link rel="stylesheet" href="/Content/css/datepicker.min.css" />

    <!-- Modernizr JS -->
    <script src="/Content/js/vendor/modernizr-2.8.3.min.js"></script>

    <style>
        div.gallery {
            border: 1px solid #ccc;
        }

            div.gallery:hover {
                border: 1px solid #777;
            }

            div.gallery img {
                width: 100%;
                height: auto;
            }

        div.desc {
            padding: 15px;
            text-align: center;
        }

        * {
            box-sizing: border-box;
        }

        .responsive {
            padding: 0 6px;
            float: left;
            width: 24.99999%;
        }

        .clearfix:after {
            content: "";
            display: table;
            clear: both;
        }
    </style>
</head>

<body>
    <!-- Preloader -->
    <div class="preloader">
        <div class="spinner">
            <div class="double-bounce1"></div>
            <div class="double-bounce2"></div>
        </div>
    </div>

    <!-- Render Body -->
    @RenderBody()

    <!-- Scripts Section -->
    @RenderSection("Scripts", required: false)

    <!-- jQuery -->
    <script src="/Content/js/vendor/jquery-1.12.0.min.js"></script>

    <!-- Bootstrap JS -->
    <script type="text/javascript" src="/Content/js/bootstrap.min.js"></script>

