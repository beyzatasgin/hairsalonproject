    <meta name="description" content="Minimal Multipurpose Portfolio Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Fav Icon -->
    <link id="logo" rel="icon" type="image/png" href="/Content/img/logom.png" />

    <!-- Google Web Fonts -->
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

    <!-- CSS Libraries -->
    <link rel="stylesheet" type="text/css" href="/Content/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="/Content/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="/Content/css/pe-icon-7-stroke.css" />
    <link rel="stylesheet" type="text/css" href="/Content/css/headlines.css" />
    <link rel="stylesheet" type="text/css" href="/Content/css/plugins.css" />
